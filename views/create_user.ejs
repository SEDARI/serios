<!DOCTYPE html>
<html>
  <head>
    <meta name="viewport" content="user-scalable=no, initial-scale=1, maximum-scale=1, minimum-scale=1, width=device-width">
    <meta charset="UTF-8">
    <title>Read User</title>
    <link rel="stylesheet" type="text/css" href="/static/css/bootstrap.css">
    <link rel="stylesheet" type="text/css" href="/static/css/style.css">
    <link href="/static/css/style.css" rel="stylesheet">
  </head>
  <body>
    <script type="text/javascript" src="/static/js/jquery-1.11.3.min.js" charset="utf-8"></script>
    <script type="text/javascript" src="/static/js/bootstrap.min.js" charset="utf-8"></script>

    <div class="content">

    <div class="container-fluid" id="wrapper">
      
      <div class="row header">
        <div class="container-fluid">
          <div class="row">
            <div class="col-xs-8 col-xs-offset-2 col-xs-10 col-xs-offset-1">
              <div class="site-heading">
              </div>
            </div>
          </div>
        </div>
      </div>
      
      <div class="row main">
        
        <div class="container-fluid">
          <h2>Register</h2>
          <div class="panel-group" id="accordion">
            
            <div class="panel panel-default">
              <div class="panel-heading">
                <h4 class="panel-title">
                  <a data-toggle="collapse" data-parent="#accordion" href="#collapse1"><b>SEDARI</b></a>
                </h4>
              </div>
              <div id="collapse1" class="panel-collapse collapse in">
                <div class="container-fluid">
                  <form class="form-horizontal" id="registerWithSEDARI"  method="post">
                    <div class="form-group">
                      <div class="row" style="margin-top:0.2em">
                        <div class="container-fluid">
                          <label for="sedari_username" class="col-sm-2 control-label">Username</label>
                          <div class="col-sm-10">
                            <input type="text" class="form-control" id="sedari_username" placeholder="username in this app" name="user_name">
                          </div>
                        </div>
                      </div>
                      <div class="row" style="margin-top:0.2em">
                        <div class="container-fluid">
                          <label for="sedari_password" class="col-sm-2 control-label">Password</label>
                          <div class="col-sm-10">
                            <input type="password" class="form-control" id="sedari_password" placeholder="password" name="password">
                          </div>
                        </div>
                      </div>
                    </div>
                    <div class="form-group">
                      <div class="row">
                        <div class="container-fluid">
                          <div class="col-sm-offset-9 col-sm-3">
                            <button type="submit" class="btn btn-primary" style="width:100%">Register</button>
                          </div>
                        </div>
                      </div>
                    </div>
                    <input type="hidden" name="role" value=""/>
                    <input type="hidden" name="auth_type" value="agile-local"/>
                  </form>
                </div>
              </div>
            </div>
            
            <div class="panel panel-default">
              <div class="panel-heading">
                <h4 class="panel-title">
                  <a data-toggle="collapse" data-parent="#accordion" href="#collapse2"><img src="/static/img/google.svg" height="30"/></a>
                </h4>
              </div>
              <div id="collapse2" class="panel-collapse collapse">
                <div class="container-fluid">
                  <form class="form-horizontal" id="registerWithGoogle" method="post">
                    <div class="form-group">
                      <div class="container-fluid">
                        <div class="row" style="margin-top:0.2em">
                          <label for="sedari_username" class="col-sm-2 control-label">Username</label>
                          <div class="col-sm-10">
                            <input type="text" class="form-control" id="sedari_username" placeholder="username in this app" name="user_name">
                            <input type="hidden" name="auth_type" value="google"/>
                          </div>
                        </div>
                      </div>
                    </div>
                    <div class="form-group">
                      <div class="container-fluid">
                        <div class="row">
                          <div class="col-sm-offset-9 col-sm-3">
                            <button type="submit" class="btn btn-primary" style="width:100%">Register</button>
                          </div>
                        </div>
                      </div>
                    </div>
                    <input type="hidden" name="auth_type" value="google"/>
                  </form>
                </div>
              </div>
            </div>
            
            <div class="panel panel-default">
              <div class="panel-heading">
                <h4 class="panel-title">
                  <a data-toggle="collapse" data-parent="#accordion" href="#collapse3"><img src="/static/img/github.png" height="30"/></a>
                </h4>
              </div>
              <div id="collapse3" class="panel-collapse collapse">
                <div class="container-fluid">
                  <form class="form-horizontal" method="post">
                    <div class="form-group">
                      <div class="container-fluid">
                        <div class="row" style="margin-top:0.2em">
                          <label for="sedari_username" class="col-sm-2 control-label">Username</label>
                          <div class="col-sm-10">
                            <input type="text" class="form-control" id="sedari_username" placeholder="username in this app" name="user_name">
                          </div>
                        </div>
                      </div>
                    </div>
                    <div class="form-group">
                      <div class="container-fluid">
                        <div class="row">
                          <div class="col-sm-offset-9 col-sm-3">
                            <button type="submit" class="btn btn-primary" style="width:100%">Register</button>
                          </div>
                        </div>
                      </div>
                    </div>
                    <input type="hidden" name="auth_type" value="github"/>
                  </form>
                </div>
              </div>
            </div>
            
            <div class="panel panel-default">
              <div class="panel-heading">
                <h2 class="panel-title">
                  <a data-toggle="collapse" data-parent="#accordion" href="#collapse4"><b>WebID</b></a>
                </h2>
              </div>
              <div id="collapse4" class="panel-collapse collapse">
                <div class="container-fluid">
                  <form class="form-horizontal" method="post">
                    <div class="form-group">
                      <div class="container-fluid">
                        <div class="row" style="margin-top:0.2em">
                          <label for="sedari_username" class="col-sm-2 control-label">Username</label>
                          <div class="col-sm-10">
                            <input type="text" class="form-control" id="sedari_username" placeholder="username in this app" name="user_name">
                          </div>
                        </div>
                      </div>
                    </div>
                    <div class="form-group">
                      <div class="container-fluid">
                        <div class="row">
                          <div class="col-sm-offset-9 col-sm-3">
                            <button type="submit" class="btn btn-primary" style="width:100%">Register</button>
                          </div>
                        </div>
                      </div>
                      <input type="hidden" name="auth_type" value="webid"/>
                    </div>
                  </form>
                </div>
              </div>
            </div>
            
          </div> <!-- Panel Group -->
        </div>
      </div> <!-- Main Row -->
    </div> <!-- Main Wrapper -->

    <div class="container-fluid">
      <div class="row footer row-centered">
        <p class="text-muted">
          <div class="col-xs-2 col-xs-offset-1"><img id="forsec_logo" class="logo" src="/static/img/forsec_logo.jpg"></div>
          <div class="col-xs-2"><img id="actlab_logo" class="logo" src="/static/img/actlab_logo.png"></div>
          <div class="col-xs-2"><img id="compose_logo" class="logo" src="/static/img/compose_logo.png"></div>
          <div class="col-xs-2"><img id="agile_logo" class="logo" src="/static/img/agile_logo.png"></div>
          <div class="col-xs-2"><img id="unipassau_logo" class="logo" src="/static/img/unipassau_logo.png"></div>
        </p>
      </div>
    </div>
    </div>
  </body>
</html>
